  ex:number_of_groups "1.0"^^xsd:integer;
  ex:official_title "Exploring the Comorbidity Between Mental Disorders and General Medical Conditions Among a Danish National Population";
  ex:plan_to_share_ipd "No";
  ex:source "University of Aarhus";
