  ex:official_title "Cytodiagnosis of Basal Cell Carcinoma and Actinic Keratosis Using Papanicolaou and May-grunvald-giemsa Stained Cutaneous Tissue Smear in Relation to Photodynamic Therapy";
  ex:source "Norwegian University of Science and Technology";
  ex:source_class "OTHER";
  ex:start_date "2000-01-31"^^xsd:dateTime;
