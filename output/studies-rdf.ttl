  ex:nct_id 40;
  ex:official_title "A Phase II Study of Flavopiridol (HMR 1275; NSC 649890) in Patients With Untreated or Relapsed Mantle Cell Lymphoma";
  ex:phase "Phase 2";
  ex:plan_to_share_ipd "No";
