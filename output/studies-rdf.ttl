  ex:number_of_groups "1.0"^^xsd:integer;
  ex:official_title "Correlation Between the Pre-transplantation Lymphocyte Count, Administered Dose of Thymoglobulin and Graft Versus Host Disease (GVHD) in Pediatric Recipients of Hematopoietic Stem Cells Transplantation: a Retrospective Study";
