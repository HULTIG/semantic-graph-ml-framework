  ex:nct_id 31;
  ex:number_of_groups "2.0"^^xsd:integer;
  ex:official_title "Does Vascular Occlusion in Liver Resections Predispose to Recurrence of Malignancy in the Liver Remnant Due to Ischemia/Reperfusion Injury?";
