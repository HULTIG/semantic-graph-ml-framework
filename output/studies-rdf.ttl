  ex:number_of_arms "2.0"^^xsd:integer;
  ex:official_title "Phase IV Study of Long Term Peg-Intron for Patients Who Have Failed to Respond to Rebetron/Interferon With Advanced Fibrosis and Cirrhosis Secondary to Hepatitis C- The Copilot Trial";
